{
  "manifest_version": 2,
  "key": "jnfcafiflcnhihjokebkadknioapkppb",
  "name": "VisionSync",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "commands" : {
    "_execute_browser_action": {
            "suggested_key": {
              "mac": "Alt+J",
              "linux": "Alt+J"
            }
          },
          "open_edit_view": {
            "suggested_key": {
              "mac": "Command+Shift+E",
              "linux": "Ctrl+Shift+E"
            },
            "description": "Open vision-sync dashboard"
          }
  },
  "permissions": [
    "storage",
    "tabs",
    "https://ajax.googleapis.com/"
  ]
}
